{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "jaDevelopment Docs",
  "colors": {
    "primary": "#346cd0",
    "light": "#6097f7",
    "dark": "#346cd0"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
