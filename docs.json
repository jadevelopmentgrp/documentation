        {
          "anchor": "Community",
          "href": "https://discord.gg/E4yHfQegnP",
          "icon": "discord"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Contact Support",
        "href": "mailto:support@jadevelopment.co.uk"
      }
